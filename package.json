{
  "name": "be-master",
  "version": "3.0.0",
  "description": "Sibaru Master",
  "main": "src/server.js",
  "repository": "https://code.nusantech.com/djp-pupr/v3/be-master",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon",
    "start": "nodemon --exec babel-node ./src/server.js",
    "setup:staging": "pm2 start ./dist/server.js --name staging-be-master",
    "setup:prod": "pm2 start ./dist/server.js --name prod-be-master",
    "serve": "node ./dist/server.js",
    "build": "npm-run-all clean transpile",
    "dev:script": "npm-run-all build serve",
    "transpile": "babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate-create": "sequelize-cli migration:generate --name",
    "start-docker": "node ./dist/server.js"
  },
  "author": "@SibaruDJP",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.420.0",
    "@aws-sdk/lib-storage": "^3.420.0",
    "@aws-sdk/s3-request-presigner": "^3.420.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.1.0",
    "chalk": "4.1.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.29.1",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.9",
    "exceljs": "^4.3.0",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "express-useragent": "^1.0.15",
    "fs": "^0.0.1-security",
    "helmet": "^5.1.1",
    "hpp": "^0.2.3",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "pug": "^3.0.2",
    "request-ip": "^3.3.0",
    "rimraf": "^3.0.2",
    "sequelize": "^6.31.1",
    "slugify": "^1.6.5",
    "winston": "^3.8.1",
    "xlsx": "^0.18.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/node": "^7.19.1",
    "@babel/preset-env": "^7.19.4",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "prettier": "^2.7.1",
    "sequelize-cli": "^6.6.0"
  },
  "nodemonConfig": {
    "exec": "npm run dev:script",
    "watch": [
      "src",
      "public"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ],
    "ext": "js, json"
  },
  "engines": {
    "node": "20.10.0"
  }
}
